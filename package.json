{
  "name": "histoire-vuetify-design-system",
  "version": "0.0.19",
  "description": "Histoire Design Systems default stories for Vuetify  - Framework and Controls",
  "license": "MIT",
  "author": "PlusTimeIT <kane@plustime.com.au> (https://plustime.com.au/)",
  "repository": {
    "url": "https://github.com/BoredLunatic/histoire-vuetify-design-system.git",
    "type": "git"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./design": "./dist/design.js"
  },
  "typesVersions": {
    "*": {
      "index": [
        "dist/index.d.ts"
      ],
      "design": [
        "dist/design.d.ts"
      ]
    }
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "rimraf dist && vite build && vue-tsc --declaration --emitDeclarationOnly -p tsconfig.build-dts.json",
    "build:types": "vue-tsc --declaration --emitDeclarationOnly",
    "watch": "vue-tsc -d -w --sourceMap"
  },
  "devDependencies": {
    "@babel/types": "^7.23.0",
    "@histoire/plugin-vue": "^0.17.1",
    "@vitejs/plugin-vue": "^4.4.0",
    "histoire": "^0.17.2",
    "rimraf": "^3.0.0",
    "typescript": "^4.9.5",
    "vue": "^3.3.4",
    "vue-router": "^4.2.5",
    "vue-tsc": "^1.8.19",
    "vuetify": "^3.3.17"
  },
  "peerDependencies": {
    "histoire": "^0.17.2",
    "vue": "^3.3.4",
    "vuetify": "^3.3.17"
  }
}
